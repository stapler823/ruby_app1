<% provide(:title, "Home") %>
<!DOCTYPE html>
<html>
  <head>
    <title><%= yield(:title) %> | SubKan</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div class="header">
      <div class="header-logo">SubKan</div>
      <div class="header-list">
        <ul>
          <li>Home</li>
          <li>User</li>
          <li>Help</li>
        </ul>
      </div>
    </div>
    <div class="main">
    </div>
    <div class="footer">
    </div>
  </body>
</html>
